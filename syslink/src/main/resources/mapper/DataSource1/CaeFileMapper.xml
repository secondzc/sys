<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tongyuan.model.dao.CaeFileMapper">
    <sql id="baseColumnList">
        id,cae_component_id,delimiters_type,file_name,template_file_name,type
    </sql>

    <insert id="add" parameterType="com.tongyuan.model.domain.CAE.CaeFile" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO
        cae_file
        (cae_component_id,delimiters_type,file_name,template_file_name,type)
        VALUES (#{caeComponentId},#{delimitersType},#{fileName},#{templateFileName},#{type})
    </insert>

</mapper>